<?xml version="1.0" encoding="UTF-8"?>
<schemalist>
        <schema path="/com/deepin/dde/deepin-pc-manager/" id="com.deepin.dde.deepin-pc-manager">
                <key name="auto-scan-usb" type="i">
                        <default>0</default>
                        <range min="0" max="1"></range>
                        <summary>Auto scan USB storage devices</summary>
                </key>
                <key name="close-type" type="i">
                        <default>0</default>
                        <range min="0" max="1"></range>
                        <summary>ask close reminder</summary>
                </key>
                <key name="ask-type" type="i">
                        <default>0</default>
                        <range min="0" max="1"></range>
                        <summary>ask close reminder</summary>
                </key>
                <key name="defender-install-time" type="s">
                        <default>'-1'</default>
                        <summary>安装时间</summary>
                </key>
                <key name="security-center-install-date" type="s">
                        <default>'-1'</default>
                        <summary>安装日期</summary>
                </key>
                <key name="safety-score" type="i">
                        <default>-1</default>
                        <range min="-1" max="100"></range>
                        <summary>上次体检得分</summary>
                </key>
                <key name="last-check-time" type="s">
                        <default>"2020-11-03-16-11-30"</default>
                        <summary>上次体检时间</summary>
                </key>
                <key name="ignore-auto-start-checking" type="b">
                        <default>false</default>
                        <summary>首页是否忽略检查自启动项</summary>
                </key>
                <key name="ignore-dev-mode-checking" type="b">
                        <default>false</default>
                        <summary>首页是否忽略检查开发者模式项</summary>
                </key>
                <key name="cleaner-last-time-cleaned" type="s">
                        <default>''</default>
                        <summary>上次垃圾清理内容大小</summary>
                </key>
                <key name="defender-palette-type" type="i">
                        <default>0</default>
			<range min="0" max="2"></range>
			<summary>主题类型，0未知类型，1浅色类型，2深色类型</summary>
                </key>
                <!--网络检测类型-->
                <key name="net-check-type" type="i">
                        <default>0</default>
                        <range min="0" max="1"></range>
                        <summary>0公网，1内网</summary>
                </key>
                <!--网络检测内网域名/ip-->
                <key name="net-check-intranet-domain" type="s">
                        <default>''</default>
                        <summary>内网域名/ip</summary>
                </key>
        </schema>
</schemalist>
